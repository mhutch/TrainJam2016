    <HasSharedItems>true</HasSharedItems>
    <SharedGUID>{841E4604-187A-4449-9981-73689779A566}</SharedGUID>
  </PropertyGroup>
  <PropertyGroup Label="Configuration">
    <Import_RootNamespace>Assets</Import_RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <UrhoAsset Include="$(MSBuildThisFileDirectory)Data\Fonts\Font.ttf">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </UrhoAsset>
    <UrhoAsset Include="$(MSBuildThisFileDirectory)Data\Models\Box.mdl">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </UrhoAsset>
    <UrhoAsset Include="$(MSBuildThisFileDirectory)Data\Models\Sphere.mdl">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </UrhoAsset>
    <UrhoAsset Include="$(MSBuildThisFileDirectory)Data\Materials\Mushroom.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </UrhoAsset>
    <UrhoAsset Include="$(MSBuildThisFileDirectory)Data\Materials\Terrain.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </UrhoAsset>
    <UrhoAsset Include="$(MSBuildThisFileDirectory)Data\Textures\TerrainDetail1.dds">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </UrhoAsset>
    <UrhoAsset Include="$(MSBuildThisFileDirectory)Data\Textures\TerrainDetail2.dds">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </UrhoAsset>
    <UrhoAsset Include="$(MSBuildThisFileDirectory)Data\Textures\TerrainDetail3.dds">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </UrhoAsset>
    <UrhoAsset Include="$(MSBuildThisFileDirectory)Data\Textures\TerrainWeights.dds">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </UrhoAsset>
    <UrhoAsset Include="$(MSBuildThisFileDirectory)Data\Textures\HeightMap.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
