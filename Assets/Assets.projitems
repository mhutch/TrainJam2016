    <UrhoAsset Include="$(MSBuildThisFileDirectory)Data\Models\Mushroom.mdl">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </UrhoAsset>
    <UrhoAsset Include="$(MSBuildThisFileDirectory)Data\Textures\Mushroom.dds">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </UrhoAsset>
    <UrhoAsset Include="$(MSBuildThisFileDirectory)Data\Materials\Stone.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </UrhoAsset>
    <UrhoAsset Include="$(MSBuildThisFileDirectory)Data\Textures\StoneDiffuse.dds">
